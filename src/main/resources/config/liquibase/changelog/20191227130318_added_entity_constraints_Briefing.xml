<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Briefing.
    -->
    <changeSet id="20191227130318-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="briefing_id"
                                 baseTableName="briefing_task"
                                 constraintName="fk_briefing_task_briefing_id"
                                 referencedColumnNames="id"
                                 referencedTableName="briefing"/>

        <addForeignKeyConstraint baseColumnNames="task_id"
                                 baseTableName="briefing_task"
                                 constraintName="fk_briefing_task_task_id"
                                 referencedColumnNames="id"
                                 referencedTableName="task"/>
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="briefing"
                                 constraintName="fk_briefing_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

    </changeSet>
</databaseChangeLog>
